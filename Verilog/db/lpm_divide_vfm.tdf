FUNCTION sign_div_unsign_8nh (denominator[10..0], numerator[15..0])
RETURNS ( quotient[15..0], remainder[10..0]);

--synthesis_resources = lut 137 
SUBDESIGN lpm_divide_vfm
( 
	denom[10..0]	:	input;
	numer[15..0]	:	input;
	quotient[15..0]	:	output;
	remain[10..0]	:	output;
) 
VARIABLE 
	divider : sign_div_unsign_8nh;
	numer_tmp[15..0]	: WIRE;

BEGIN 
	divider.denominator[] = denom[];
	divider.numerator[] = numer_tmp[];
	numer_tmp[] = numer[];
	quotient[] = divider.quotient[];
	remain[] = divider.remainder[];
END;
